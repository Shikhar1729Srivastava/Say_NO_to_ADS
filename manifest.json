{
    "manifest_version": 3,
    "name": "Simple Ad Blocker",
    "version": "1.0.2",
    "permissions": [
      "declarativeNetRequest",
      "activeTab",
      "storage"
    ],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "index.html"
    },
    "host_permissions": [
      "*://*/*"
    ],
    "icons": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
  }
  
